{
  "name": "personal-site",
  "description": "thejackobrien personal site",
  "version": "1.0.0",
  "private": true,
  "author": "Jack O'Brien",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
